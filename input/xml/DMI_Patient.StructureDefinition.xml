<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <url value="http://esante.gouv.fr/ci-sis/fhir/StructureDefinition/DMI_Patient" />
  <name value="DMI_Patient" />
  <status value="draft" />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Patient" />
  <baseDefinition value="http://interopsante.org/fhir/StructureDefinition/FrPatient" />
  <derivation value="constraint" />
  <differential>
    <element id="Patient.meta">
      <path value="Patient.meta" />
      <min value="1" />
    </element>
    <element id="Patient.identifier">
      <path value="Patient.identifier" />
      <min value="1" />
    </element>
    <element id="Patient.telecom">
      <path value="Patient.telecom" />
      <min value="1" />
    </element>
    <element id="Patient.telecom.system">
      <path value="Patient.telecom.system" />
      <binding>
        <strength value="required" />
        <valueSet value="https://mos.esante.gouv.fr/NOS/TRE_R200-CanalCommunication/" />
      </binding>
    </element>
    <element id="Patient.address.type">
      <path value="Patient.address.type" />
      <binding>
        <strength value="required" />
        <valueSet value="https://mos.esante.gouv.fr/NOS/TRE_R35-TypeVoie/" />
      </binding>
    </element>
  </differential>
</StructureDefinition>